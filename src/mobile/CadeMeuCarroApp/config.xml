<?xml version='1.0' encoding='utf-8'?>
<widget id="org.deves.cademeucarro" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cadê meu Carro?</name>
    <description>
        Projeto aberto da comunidade Dev-ES com a finalidade de criar um serviço de rastreio geográfico de carros roubados.
    </description>
    <author email="edgar.vv@gmail.com" href="http://edgarberlinck.github.io">
        Edgar Muniz Berlinck
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
